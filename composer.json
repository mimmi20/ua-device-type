{
  "name": "mimmi20/ua-device-type",
  "type": "library",
  "description": "a class collection for different device types",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-device-type",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-device-type/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "mimmi20/browser-detector-loader-interface": "~3.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "infection/infection": "^0.12.0",
    "localheinz/composer-normalize": "^1.1.0",
    "localheinz/phpstan-rules": "^0.7 || ^0.8",
    "pepakriz/phpstan-exception-rules": "^0.6 || ^0.7",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^7.5 || ^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaDeviceType\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaDeviceTypeTest\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ua-device-type/issues",
    "source": "https://github.com/mimmi20/ua-device-type"
  }
}
