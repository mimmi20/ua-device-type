{
  "name": "mimmi20/ua-device-type",
  "description": "a class collection for different device types",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-device-type/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-device-type",
  "support": {
    "issues": "https://github.com/mimmi20/ua-device-type/issues",
    "source": "https://github.com/mimmi20/ua-device-type"
  },
  "require": {
    "php": "~8.4.0 || ~8.5.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.32.3",
    "jbelien/phpstan-sarif-formatter": "^1.2.0",
    "mimmi20/coding-standard": "^7.0.20",
    "nikic/php-parser": "^5.7.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.37",
    "phpstan/phpstan-deprecation-rules": "^2.0.3",
    "phpstan/phpstan-phpunit": "^2.0.12",
    "phpunit/phpunit": "^12.5.7",
    "rector/rector": "^2.3.4",
    "rector/type-perfect": "^2.1.2",
    "shipmonk/composer-dependency-analyser": "^1.8.4",
    "symfony/process": "^8.0.4",
    "symplify/phpstan-rules": "^14.9.11",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.1.0",
    "tomasvotruba/unused-public": "^2.2.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "UaDeviceType\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaDeviceTypeTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
