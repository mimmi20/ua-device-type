{
  "name": "mimmi20/ua-device-type",
  "type": "library",
  "description": "a class collection for different device types",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-device-type",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-device-type/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "mimmi20/browser-detector-loader-interface": "~3.0.6"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.5.1",
    "ergebnis/phpstan-rules": "^0.15.0",
    "infection/infection": "^0.13.6 || ^0.16.3",
    "mimmi20/coding-standard": "~1.0.0",
    "pepakriz/phpstan-exception-rules": "^0.11.0",
    "phpstan/phpstan": "^0.12.25",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.8",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^7.5.20 || ^8.5.4 || ^9.1.3"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaDeviceType\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaDeviceTypeTest\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ua-device-type/issues",
    "source": "https://github.com/mimmi20/ua-device-type"
  }
}
