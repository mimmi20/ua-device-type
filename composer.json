{
  "name": "mimmi20/ua-device-type",
  "type": "library",
  "description": "a class collection for different device types",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-device-type",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-device-type/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-mbstring": "*",
    "mimmi20/browser-detector-loader-interface": "^4.0.1"
  },
  "require-dev": {
    "infection/infection": "^0.21.5",
    "mimmi20/coding-standard": "^2.1.1",
    "pepakriz/phpstan-exception-rules": "^0.11.6",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.83",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpunit/phpunit": "^9.5.4"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaDeviceType\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaDeviceTypeTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/mimmi20/ua-device-type/issues",
    "source": "https://github.com/mimmi20/ua-device-type"
  }
}
